<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.udacodingweek2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".DetailMakananActivity"
            android:parentActivityName=".MakananActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MakananActivity" />
        </activity>
        <activity android:name=".MakananActivity"
            android:parentActivityName=".MakananActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".WisataActivity"
                />
        </activity>
        <activity
            android:name=".DetailWisata"
            android:parentActivityName=".WisataActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".WisataActivity" />
        </activity>

        <activity android:name=".WisataActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".LoginSederhanaActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".HitungBeratBadanActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity android:name=".HitungUmurActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>